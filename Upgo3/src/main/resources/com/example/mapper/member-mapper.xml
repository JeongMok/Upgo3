	<select id="selectMemberByIdAndPasswd" parameterType="hashmap"
		resultType="com.example.dto.Member">
		SELECT * 
		FROM MEMBER 
		WHERE MBRID=#{mbrId} AND MBRPASSWD=#{mbrPasswd}
	</select>
	
	<insert id="insertMember" parameterType="com.example.dto.Member">
		INSERT INTO MEMBER (MBRID, MBRPASSWD, MBRNAME, MBRPHONE, MBRADDRESS, MBRUSERTYPE)
		VALUES (#{mbrId}, #{mbrPasswd}, #{mbrName}, #{mbrPhone}, #{mbrAddress}, #{mbrUserType})
	</insert>

</mapper>








