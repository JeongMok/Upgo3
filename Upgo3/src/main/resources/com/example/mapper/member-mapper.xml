<mapper namespace="com.example.mapper.MemberMapper">

	<!-- <resultMap type="com.example.dto.Member" id="MemberMap">
		<id column="MBRID" property="mbrId" />
		<result column="MBRPASSWD" property="mbrPasswd" />
		<result column="MBRNAME" property="mbrName" />
		<result column="MBRPHONE" property="mbrPhone" />
